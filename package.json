{
  "name": "trello",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "globstar nu-html-checker *.html",
    "init:dirs": "mkdir sass css vendor images js",
    "init:files": "touch index.html sass/style.scss js/script.js",
    "build": "npm-run-all build:*",
    "build:clean": "globstar rimraf css/*.css*",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch:browsersync": "browser-sync start --server --files css/*.css *.html",
    "watch:sassprefixer": "onchange -d 100 sass/*.scss -- npm-run-all watch:sass watch:autoprefixer",
    "watch:sass": "node-sass --output-style compact -o css sass",
    "watch:autoprefixer": "autoprefixer-cli css/style.css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.24.6",
    "globstar": "^1.0.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "nu-html-checker": "^0.1.0",
    "onchange": "^4.1.0",
    "rimraf": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mruminski/trello.git"
  },
  "bugs": {
    "url": "https://github.com/mruminski/trello/issues"
  },
  "homepage": "https://github.com/mruminski/trello#readme"
}
